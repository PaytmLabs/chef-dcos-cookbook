---
<% if not node["dcos"]["agent_list"].empty? %>
agent_list:
<% node["dcos"]["agent_list"].each do |ip| %>
  - <%= ip %>
<% end %>
<% end %>
bootstrap_url: <%= node["dcos"]["bootstrap_url"] %>
cluster_name: <%= node["dcos"]["cluster_name"] %>
<% if not node["dcos"]["customer_key"].nil? %>
customer_key: <%= node["dcos"]["customer_key"] %>
<% end %>
# TODO: support ZK and S3 storage backends -ccampo 2016-04-26
exhibitor_storage_backend: static
# TODO: support VRRP master discovery -ccampo 2016-04-26
master_discovery: static
master_list:
<% node["dcos"]["master_list"].each do |ip| %>
  - <%= ip %>
<% end %>
<% if not node["dcos"]["dns_search"].empty? %>
dns_search: <%= node["dcos"]["dns_search"].join(' ') %>
<% end %>
resolvers:
<% node["dcos"]["resolvers"].each do |ip| %>
  - <%= ip %>
<% end %>
process_timeout: <%= node["dcos"]["process_timeout"] %>
oauth_enabled: '<%= node["dcos"]["oauth_enabled"] %>'
telemetry_enabled: '<%= node["dcos"]["telemetry_enabled"] %>'
ssh_port: <%= node["dcos"]["ssh_port"] %>
ssh_user: <%= node["dcos"]["ssh_user"] %>
